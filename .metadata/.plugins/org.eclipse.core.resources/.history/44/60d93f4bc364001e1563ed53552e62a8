package com.sathish;

import org.springframework.context.annotation.Bean;

public class AppConfig {
	
	@Bean
	public Student getStudent() {
		return new Student();
	}
	@Bean("myst")
	public Student getStudents() {
		return new Student();
	}
	// Student mystu=new Student();
	
	@Bean
	public Book createbook() {
		return new Book();
	}

}
